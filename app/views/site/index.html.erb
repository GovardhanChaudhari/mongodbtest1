<%= link_to "upload", "/showupload"%>
<%=form_tag("/search",:method=>"get	") do	%>
	
	<%= hidden_field_tag("attempt_id",@attempt_id)%>
	<% @options.to_a.each_index do |index| %>
		<% @option_index = index %>
		<%= render 'college_options' %>
	<%end%>
	
	Caste: <%= select_tag("caste",options_for_select(Cutoff.all.map{|co| [co.caste,co.caste]}.uniq))%><br>
	Exam Type: <%= select_tag("exam_type",options_for_select(EXAM_TYPES,EXAM_TYPES))%><br>
	Mark_type: <%= select_tag("marks_type",options_for_select(MARK_TYPES,MARK_TYPES))%><br>
	From : <%= text_field_tag ("score_value_from")%><br>
	To: <%= text_field_tag("score_value_to")%><br>
	<%= submit_tag("Search") %>
<%end%>
